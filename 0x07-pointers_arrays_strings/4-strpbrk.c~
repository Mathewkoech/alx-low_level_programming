11;rgb:0000/0000/0000#include <stdio.h>
/**
 * _strpbrk - Searches a string for any of a set of bytes.
 * @s:source
 * @accept:Acceptable characters.
 *
 * Return: 's',or NULL if no such character is found.
 */
char *_strpbrk(char *s, char *accept)
{
while (*s != '\0')
{
char *a = accept;

while (*a != '\0')
{
if (*s == *a)
{
return (s);
}
a++;
}

s++;
}

return (NULL);
}
