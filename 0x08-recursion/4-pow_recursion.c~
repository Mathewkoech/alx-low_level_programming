#include "main.h"
0;276;0c/**
 *_pow_recursion-  function that returns the value of x
 * raised to the power of y
 *@x:base
 *@y:exponent
 * Return:returns the value of x raised to the power of y
 */

int _pow_recursion(int x, int y)
{
	if (y < 0)

	{
		return (-1);
	}
	else if (y == 0)
	{
		return (0);
	}
	return (x * _pow_recursion(x, y - 1));
}
